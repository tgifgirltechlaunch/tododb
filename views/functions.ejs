<% getCatImg = function(currcat) {
    var imgpath = "" ;
    if (currcat === "misc") {
        imgpath ='misc.svg';
    } 
    if (currcat === "hobby") {
        imgpath ='hobby.svg';
    } 
    if (currcat === "work") {
        imgpath ='work.svg';
    } 
    if (currcat === "leisure") {
        imgpath ='leisure.svg';
    } 
    if (currcat === "home") {
        imgpath ='home.svg';
    } 
    return imgpath;
} %>